https://leetcode.com/explore/learn/card/fun-with-arrays/526/deleting-items-from-an-array/3247/


class Solution {
public:
    int removeElement(vector<int>& nums, int val) {
        int start = 0;
        for(int i = 0; i < nums.size(); i++){
            if(nums[i] != val) nums[start++] = nums[i];
        }
         return start;
    }
};
